    "dev": "next dev",
    "dev:https": "node server.js",
    "build": "next build",
    "start": "next start",
    "start:https": "NODE_ENV=production node server.js",
    "lint": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "ci": "npm run type-check && npm run lint && npm run test",
    "prepare": "husky",
