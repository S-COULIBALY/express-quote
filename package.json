    "generate:fallbacks": "ts-node scripts/generate-fallbacks.ts",
    "notification:setup": "bash scripts/setup-notification-system.sh",
    "notification:docker:up": "docker-compose -f config/docker-compose.notification.yml up -d",
    "notification:docker:down": "docker-compose -f config/docker-compose.notification.yml down",
    "notification:docker:logs": "docker-compose -f config/docker-compose.notification.yml logs -f",
    "whatsapp:test": "node scripts/test-whatsapp.js",
    "whatsapp:validate": "node scripts/validate-whatsapp-credentials.js",
    "whatsapp:send": "node scripts/test-whatsapp.js",
    "pre-prod-check": "ts-node scripts/pre-production-check.ts",
    "pre-prod-check:skip-tests": "ts-node scripts/pre-production-check.ts --skip-tests",
    "pre-prod-check:fast": "ts-node scripts/pre-production-check.ts --skip-tests --skip-lint",
    "project-status": "ts-node scripts/project-status.ts",
    "clean": "ts-node scripts/clean-project.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\""
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
